public class DogTest {
    public static void main(String args[]){
        int[] even= new int[5];
        int j=0;


        for (int i=1; i<11; i++){


            if (i%2==0){
                even[j]=i;
                j++;    
            }
            else {
                continue;
            }
        }


        for (int d : even){
            System.out.println(d);
        }


    }
}


